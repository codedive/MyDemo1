@{
    ViewBag.Title = "URFX | Help";
}
<header>
    <div class="navbar-wrapper">
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                    <a class="navbar-brand" href="/"><img src="/Content/images/logo.png" alt="logo-here" ng-hide="showArabicLogo"><img src="~/Content/images/arabic-logo.png" ng-show="showArabicLogo" /></a>

                </div>
                <div id="navbar" class="navbar-collapse collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a class="page-scroll" href="/App"><span translate>Login</span></a></li>
                        <li>
                            <a class="btn language" validation-reset="loginForm" ng-click="setLang('en', $event)" title="English">English</a>

                        </li>
                        <li><a class="btn language2" validation-reset="loginForm" ng-click="setLang('ar', $event)" title="العربية">العربية</a></li>


                    </ul>
                </div>
            </div>
        </nav>
    </div>

</header>
<div class="innercontent">
    <div class="container">
        <div class="row">
            <div class="contentheading">
                <h2 translate>Help</h2>
            </div>
            <div class="inner-contentbox help chosen-rtl" dir="rtl">                
                <p>يرجى التواصل معنا على البريد الالكتروني المدون ادناة:</p>
                <p>support@urfxco.com</p>
            </div>
        </div>
    </div>
</div>


