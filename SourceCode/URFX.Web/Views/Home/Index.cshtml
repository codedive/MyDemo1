@{
    Layout = null;
}
<!DOCTYPE html>
<html ng-app="UrfxApp">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <meta name="viewport" content="minimum-scale=1.0; maximum-scale=1.0; user-scalable=false; initial-scale=1.0;" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=1, minimum-scale=1.0, maximum-scale=1.0">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,100,100italic,300italic,500,400italic,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
     <title ng-bind="pageTitle"></title>
    <meta name="fragment" content="!">
   
    <base href="/" />
    <script>
$(function () {
  $('[data-toggle="popover"]').popover()
})</script>
    <!-- Bootstrap -->    
    <link href="~/Content/URFXTheme/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/URFXTheme/css/style.css" rel="stylesheet" />
    <link href="~/Content/URFXTheme/css/responsive.css" rel="stylesheet" />    
    <link href="~/Content/URFXTheme/css/font-awesome.min.css" rel="stylesheet" /> 
    <link href="~/Content/URFXTheme/css/animate.css" rel="stylesheet" />      
    <link href="~/Content/URFXTheme/css/simple-sidebar.css" rel="stylesheet" />
    <link href="~/Content/URFXTheme/css/sb-admin.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]--> 
  
    @*@Styles.Render("~/Content/css")*@    
</head>
<body ng-class="bodyClass">
    <ui-view></ui-view>
    <!-- JQUERY -->
    @Scripts.Render("~/bundles/jquery")

    <!-- BOOTSTRAP -->
    @Scripts.Render("~/bundles/bootstrap")

    <!-- ANGULAR -->
    @Scripts.Render("~/bundles/angular")

    <!-- ANGULAR UI -->
    @*@Scripts.Render("~/bundles/angular-ui")*@

    <!-- APP -->
    @Scripts.Render("~/bundles/app")

    <!-- SERVICES -->
    @Scripts.Render("~/bundles/Services")

    <!-- CONTROLLERS -->
    @Scripts.Render("~/bundles/Controllers")

    <!-- DIRECTIVES-->
    @Scripts.Render("~/bundles/Directives")

    <script type="text/javascript" src="~/Scripts/angular-cookies.js"></script>
    <script type="text/javascript" src="~/Scripts/angular-translate.js"></script>
    <script type="text/javascript" src="~/Scripts/angular-translate-loader-url.js"></script>    
    <script type="text/javascript" src="~/app/Services/rememberService.js"></script>
</body>
</html>